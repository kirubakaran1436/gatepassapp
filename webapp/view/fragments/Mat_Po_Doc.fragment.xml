<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog id="PoDocHead_dialog" noDataText="No Purchase Document Found" title="Select Purchase Document" search=".OnPoDocHeadSearch" confirm=".OnPoDcoHeadSelect"
		contentWidth="50%" cancel=".OnPoDcoHeadSelect" items="{JModel>/data}">
		<ColumnListItem id="_IDGenColumnListItem1" vAlign="Middle">
			<cells>
				<Text id="_IDGenText1" text="{JModel>PurchaseOrder}"/>
				<Text id="_IDGenText2" text="{JModel>Supplier}"/>
				<Text id="_IDGenText3" text="{JModel>SupplierName}"/>
				<Text visible="false" id="_IDGenText4" text="{JModel>Supplier}"/>
				<Text visible="false" id="_IDGenText45" text="{JModel>Plant}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column id="_IDGenColumn1" >
				<header>
					<Text id="_IDGenText31" text="Purchase Order"/>
				</header>
			</Column>
			<Column id="_IDGenColumn2" >
				<header>
					<Text id="_IDGenText5" text="Vendor Code"/>
				</header>
			</Column>
			<Column id="_IDGenColumn3" >
				<header>
					<Text id="_IDGenText41" text="Vendor Name"/>
				</header>
			</Column>
			<Column visible="false" id="_IDGenColumn45" >
				<header>
					<Text id="_IDGenText6" text="Plant"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>


